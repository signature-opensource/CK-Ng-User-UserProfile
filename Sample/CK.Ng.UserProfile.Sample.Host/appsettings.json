{
    "AllowedHosts": "*",
    "Authentication": {
        "Oidc.SignatureAD": {
            "Instance": "https://login.microsoftonline.com/",
            "ClientId": "",
            "TenantId": "",
            "ClientSecret": "",
            "CallbackPath": "/signin-oidc-signature",
            "SignedOutCallbackPath ": "/signout-callback-oidc-signature",
            "RequireHttpsMetadata": false
        }
    },
    "CK-AppIdentity": {
        "FullName": "Sample/$Server",
        "Parties": []
    },
    "CK-Monitoring": {
        "LogPath": "Logs",
        "HandleDotNetLogs": true,
        "DefaultFilter": "Debug",
        "GrandOutput": {
            "Handlers": {
                "Console": true,
                "TextFile": {
                    "Path": "Text"
                }
            }
        }
    },
    "Kestrel": {
        "Endpoints": {
            "Http": {
                "Url": "http://127.0.0.1:5000"
            },
            "Http6": {
                "Url": "http://[::]:5000"
            }
        }
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Warning"
        },
        "Console": {
            "LogLevel": {
                "Default": "None"
            },
            "FormatterOptions": {
                "IncludeScopes": false
            }
        },
        "Debug": {
            "LogLevel": {
                "Default": "None"
            }
        }
    }
}
